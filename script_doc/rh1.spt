FUNC RHINLE45OUTLG45
	SENSOR  $IN3IN1SENSORID sensorData humidity TH [45,]
	SENSOR  $OUT3IN1SENSORID sensorData humidity TH [,45],

FUNC RHINLG75OUTLE75
	SENSOR  $IN3IN1SENSORID sensorData humidity TH [,75]
	SENSOR  $OUT3IN1SENSORID sensorData humidity TH [75,],

FUNC RHINEQ4575
	SENSOR  $IN3IN1SENSORID sensorData humidity TH [45,75],


FUNC REFRESHHUMIDAUTO
	ALARM RHINLE45OUTLG45 REFRESHFANON
	ALARM RHINLG75OUTLE75 REFRESHFANON
	ALARM RHINEQ4575 REFRESHFANOFF,

TIME LOOP REFRESHHUMIDAUTO 600
