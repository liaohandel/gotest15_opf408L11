FUNC RHON 
	RELAY $RHID ON,
FUNC RHOFF 
	RELAY $RHID OFF,

FUNC RHINLE45OUTLE45
	SENSOR  $IN3IN1SENSORID sensorData humidity TH [45,],
	SENSOR  $OUT3IN1SENSORID sensorData humidity TH [45,],

FUNC RHINLG45
	SENSOR  $IN3IN1SENSORID sensorData humidity TH [,45],

FUNC RHAUTO
	ALARM RHINLE45OUTLE45 RHON
	ALARM RHINLG45 RHOFF

TIME LOOP RHAUTO 600
